                        function tracking() {
                var post_id = $(".post_id").val();
                var utm_source = $(".utm_source").val();
                var utm_medium = $(".utm_medium").val();
                var t = $.now();
                $.ajax({
                    url: '/ajax/tracking',
                    params: {
                        _token: "SqvCzCQLyeGMmA9Rg26rXZHRjvyfnQfvV5T2UUNX"
                    },
                    headers: {
                        'X-CSRF-TOKEN': "SqvCzCQLyeGMmA9Rg26rXZHRjvyfnQfvV5T2UUNX"
                    },
