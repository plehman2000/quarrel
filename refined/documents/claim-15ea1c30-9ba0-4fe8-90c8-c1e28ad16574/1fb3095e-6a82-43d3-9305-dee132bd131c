                        post_id : post_id,
                        utm_medium : utm_medium,
                        utm_source : utm_source,
                        t : t
                    },
                    method: 'GET',
                    dataType: 'json',
                    success: function(data){
                    }
                });
            }
            setTimeout(tracking, 5000);

