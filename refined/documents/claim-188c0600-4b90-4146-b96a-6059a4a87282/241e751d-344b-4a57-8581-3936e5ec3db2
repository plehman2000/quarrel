<script type="text/javascript">
try {
window.modalSettings = {"show":true,"force":false,"title":"Get 3 Months of GINX+ On Us","image":"\/uploads3\/1 - Model Pop Up\/GINX_PLUS_POP_UP.png","text":"We're making GINX+ video content FREE until December 2024. Sign up today for access to our 24\/7 live TV channel, exclusive new episodes of The First Hour and our existing video library!","button":"Click Here To Start Your Trial","url":"https:\/\/www.ginx.tv\/en\/promolink\/4_5e8c69f540fed62c"};
} catch(e) {
window.modalSettings = { show: false, title: '', image: '', text: '', button: '', url: '' };
}
</script>
<script type="text/javascript" src="/compiled/js/public/js/base.js?av_20241104_144118" async=""></script>
<script type="text/javascript" data-cfasync="false">
(function() {
let isNextArticleLoading = false;
let nextPost = "/en/minecraft/live-2024-date-september-28";
let loadedPosts = [document.body.getAttribute('data-post-lang-id')];
const articleScrollObserver = new IntersectionObserver((entries) => {
entries.forEach(entry => {
if (entry.isIntersecting) {
const post = entry.target;
const url = post.getAttribute('data-url');
if (url) {
history.pushState({}, '', url);
// Trigger page view on google tag manager
gtag('config', 'G-C1KQYE3N3Y', {
'page_path': url
});
dataLayer.push({
'event': 'pageview',
'pagePath': url
});
// Trigger page view on matomo
if (typeof _paq !== 'undefined') {
_paq.push(['setCustomUrl', url]);
_paq.push(['trackPageView']);
}
}
//console.log('Intersecting article:', post.getAttribute('data-url'));
}
});
});
const handleAccordions = () => {
document.querySelectorAll('.accordion-box-title').forEach(item => {
if (!item.hasAttribute('data-ready')) {
item.addEventListener('click', (event) => {
const accordionBox = event.currentTarget.parentNode;
if (accordionBox.classList.contains('expanded')) {
accordionBox.classList.remove('expanded');
} else {
accordionBox.classList.add('expanded');
}
});
item.setAttribute('data-ready', '1');
}
});
};
const handleImageGalleries = () => {
document.querySelectorAll('.rsp-imagegallery.gallery-slider').forEach(item => {
if (!item.hasAttribute('data-ready')) {
// Count elements inside item
const figures = item.querySelectorAll('figure');
item.setAttribute('data-current', '0');
item.setAttribute('data-last', figures.length === 1 ? '1' : '0');
const arrowPrevious = document.createElement('div');
arrowPrevious.setAttribute('class', 'rsp-imagegallery__arrow rsp-imagegallery__arrow--prev');
arrowPrevious.addEventListener('click', () => {
const currentIndex = parseInt(item.getAttribute('data-current'));
if (currentIndex > 0) {
item.setAttribute('data-current', (currentIndex - 1) + '');
figures.forEach(figure => {
figure.style.transform = 'translateX(' + ((currentIndex - 1) * -100) + '%)';
});
item.setAttribute('data-last', '0');
}
});
const arrowNext = document.createElement('div');
arrowNext.setAttribute('class', 'rsp-imagegallery__arrow rsp-imagegallery__arrow--next');
arrowNext.addEventListener('click', () => {
const currentIndex = parseInt(item.getAttribute('data-current'));
if (currentIndex < figures.length - 1) {
item.setAttribute('data-current', (currentIndex + 1) + '');
figures.forEach(figure => {
figure.style.transform = 'translateX(' + ((currentIndex + 1) * -100) + '%)';
});
}
if (currentIndex === figures.length - 2) {
item.setAttribute('data-last', '1');
} else {
item.setAttribute('data-last', '0');
}
});
item.appendChild(arrowPrevious);
item.appendChild(arrowNext);
item.setAttribute('data-ready', '1');
}
});
}
const loadNextArticle = () => {
if (isNextArticleLoading || !nextPost || nextPost === '') {
//console.log("Aborted loading next article | isNextArticleLoading =", isNextArticleLoading, "| nextPost =", nextPost, "| loadedPosts length =", loadedPosts.length);
return;
}
//console.log("Starts loading next article");
isNextArticleLoading = true;
const params = new URLSearchParams();
params.append('standalone', 'true');
params.append('loadedPosts', loadedPosts.sort().join(','));
const xhr = new XMLHttpRequest();
xhr.open('GET', nextPost + '?' + params.toString(), true);
xhr.setRequestHeader('X-Requested-With', 'XMLHttpRequest');
xhr.onreadystatechange = () => {
if (xhr.readyState === 4 && xhr.status === 200) {
// Append the next article
const nextArticle = document.createElement('article');
nextArticle.innerHTML = xhr.responseText;
// Add before iscroll-anchor
document.getElementById('iscroll-anchor').before(nextArticle);
// Handle accordions and image galleries
handleAccordions();
handleImageGalleries();
observeForArticleScroll();
// Reset the flag from header X-Next-Post
nextPost = xhr.getResponseHeader('X-Next-Post');
loadedPosts.push(parseInt(xhr.getResponseHeader('X-Post-Lang-Id')));
isNextArticleLoading = false;
//console.log("Loaded next article. Next:", nextPost);
}
};
xhr.send();
};
/**
* Detect when the scroll reaches iscroll-anchor
*/
const observeForNextArticle = () => {
const iscrollAnchor = document.getElementById('iscroll-anchor');
const observer = new IntersectionObserver((entries) => {
if (entries[0].isIntersecting) {
// Trigger the next article
loadNextArticle();
}
});
observer.observe(iscrollAnchor);
};
// Detect when we scorll into a new <article data-type="post" data-url="xxx"> and update the browser url:
const observeForArticleScroll = () => {
document.querySelectorAll('article[data-type="post"]').forEach(article => {
articleScrollObserver.unobserve(article);
articleScrollObserver.observe(article);
});
};
handleAccordions();
handleImageGalleries();
observeForNextArticle();
observeForArticleScroll();
})();
// Expand table of contents
// If list items more than 3, hide all list items except the first 3 items and add an expand button to the bottom.
document.addEventListener('DOMContentLoaded', function() {
// Select the .mce-toc element
let tocElement = document.querySelector('.mce-toc');
if (tocElement) {
let listItems = tocElement.querySelectorAll('li');
if (listItems.length > 3) {
for (let i = 3; i < listItems.length; i++) {
listItems[i].style.display = 'none';
}
let expandDiv = document.createElement('div');
expandDiv.setAttribute('class', 'mce-toc-expand');
expandDiv.innerHTML = '<a href="#" style="color: #0a0a0a;">click to expand</a>';
tocElement.appendChild(expandDiv);
// Add event listener to the expand button
expandDiv.addEventListener('click', function(event) {
event.preventDefault();
for (let i = 3; i < listItems.length; i++) {
listItems[i].style.display = '';
}
expandDiv.style.display = 'none';
});
}
}
});
</script>
