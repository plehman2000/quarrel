    window.eventTracker = window.eventTracker || {};
    eventTracker.cmd = eventTracker.cmd || [];

    var eventMapping = {
      revampLoaded: { ga4_di_status: 'revamp_loaded' },
      flowInitiated: { ga4_di_status: 'flow_initiated' },
      diDisabledByMeta: { ga4_di_status: 'disabled_by_meta' },
      diHasBeenShown: { ga4_di_status: 'has_been_shown' },
      diHasNotPriority: { ga4_di_status: 'has_not_priority' },
      diDisabledForPageType: { ga4_di_status: 'disabled_for_page_type' },
      diDisabledInvalidPublisherConfig: { ga4_di_status: 'disabled_invalid_publisher_config' },
      gettingContext: { ga4_di_status: 'getting_context' },
      getCampaign: { ga4_event: 'di_request' },
      diErrorRenderingRootElement: { ga4_di_status: 'error_rendering_root_element' },
      diSetModalVisible: { ga4_di_status: 'set_modal_visible' },
      diPreRenderDone: { ga4_di_status: 'pre_render_done' },
      close: { UaName: 'Close', ga4_event: 'di_click', ga4_clicked: 'close' },
      continue: { UaName: 'ClickContinue', ga4_event: 'di_click', ga4_clicked: 'continue' },
      display: { UaName: 'Show', ga4_event: 'di_show' },
      downloadRecommended: { UaName: 'ClickProgram', ga4_event: 'di_download', download_type: 'download' },
      downloadLandingPage: { UaName: 'ClickExternalProgram', ga4_event: 'di_download', download_type: 'download_visit_website' },
      home: { UaName: 'ClickHome', ga4_event: 'di_click', ga4_clicked: 'home' },
      support: { UaName: 'ClickSupportPage', ga4_event: 'di_click', ga4_clicked: 'support' },
      error: {
        UaName: 'Error',
        labelFn: (ev) => ev.error || 'unknown error',
        nonInteractive: true,
      }
    };

    function trackUAEvent(action, label, nonInteraction) {
      eventTracker.cmd.push(function() {
        eventTracker.trackEvent("generic", {
          category: "RecommendedAppPopup",
          action,
          label,
          nonInteraction,
        });
      });
    }

    function trackGA4Event(diEvent) {
      var eventMapped = eventMapping[diEvent.kind];
      if (!eventMapped) {
          return;
      }

      if (diEvent.kind === 'error') {
        window.dataLayer.push({
          event: 'di_error',
          error_message: diEvent.error || 'unknown error',
          di_status: 'error',
        });
      } else if (diEvent.kind === 'getCampaign') {
        const suitableCampaigns = (diEvent.campaignsResult?.suitableCampaigns || []).map(({ id }) => id);
        const winnerCampaign = diEvent.campaignsResult?.winnerCampaign?.id ? 'has_winner_campaign' : 'has_not_winner_campaign';
        window.dataLayer.push({
          event: eventMapped.ga4_event,
          detected_country: diEvent.countryCode,
          suitable_campaigns: suitableCampaigns,
          di_status: suitableCampaigns.length === 0 ? 'non_suitable_campaign' : winnerCampaign,
        });
      } else if (['revampLoaded', 'flowInitiated', 'diDisabledByMeta', 'diHasBeenShown', 'diHasNotPriority', 'diDisabledForPageType', 'diDisabledInvalidPublisherConfig', 'gettingContext', 'diErrorRenderingRootElement', 'diPreRenderDone', 'diSetModalVisible' ].includes(diEvent.kind)) {
        window.dataLayer.push({
          di_status: eventMapped.ga4_di_status,
        });
      } else {
        window.dataLayer.push({
          event: eventMapped.ga4_event,
          ...(eventMapped.ga4_clicked && { element_clicked: eventMapped.ga4_clicked }),
          ...(eventMapped.download_type && { di_download_type: eventMapped.download_type }),
          di_campaign_name: diEvent.campaign?.id ? diEvent.campaign.id : 'n/a',
          campaign_delivery_logic: diEvent.campaign.logic,
          detected_country: diEvent.countryCode,
        });
      }
    }

    Object.keys(eventMapping).forEach((eventName) => {
      document.addEventListener(`rv:di:${eventName}`, (ev) => {
        var diEvent = ev.detail;
        var eventMapped = eventMapping[diEvent.kind];
        if (!eventMapped) {
          return;
        }
        var campaignIdLabelFn = (ev) => ev.campaign?.id ? ev.campaign?.id : 'n/a';
        var labelFn = eventMapped.labelFn ? eventMapped.labelFn : campaignIdLabelFn;
        if (diEvent.campaign) {
          window.dataLayer.push({
            cpiSelectionLogic: diEvent.campaign.logic,
            diCampaign: diEvent.campaign.id
          });
        }
        if (eventMapped.UaName){
          trackUAEvent(
            eventMapped.UaName,
            labelFn(diEvent),
            !!eventMapped.nonInteractive
          );
        }
        trackGA4Event(diEvent);
      });
    });
  })();
