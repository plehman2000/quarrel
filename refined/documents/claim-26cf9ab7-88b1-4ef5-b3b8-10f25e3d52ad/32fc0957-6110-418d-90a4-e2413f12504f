  <!-- title -->
  <h1 class="article-header__title">
    Minecraft creator Dream accused of grooming by 2 former fans  </h1>

  <!-- subtitle -->
      <h2 class="article-header__subtitle">
      Dream denied the allegations.    </h2>
  
  <!-- author -->
  <div class="article-header__info">
        <a href="https://www.dailydot.com/author/steven-asarch/" class="article-header__info__author" data-mrf-recirculation="Author Profile Links" data-mrf-link="https://www.dailydot.com/author/steven-asarch/" cmp-ltrk="Author Profile Links" cmp-ltrk-idx="0" mrfobservableid="1ed251d3-e77e-46f3-802a-dc815339877c">
      <img data-del="avatar" alt="Photo of Steven Asarch" src="https://uploads.dailydot.com/2022/04/steven-cheddar.webp?auto=compress&amp;fit=fit&amp;fm=jpg&amp;h=96&amp;w=96" class="avatar pp-user-avatar avatar-96 photo article-header__info__author__avatar" height="96" width="96" pinger-seen="true">      <p class="article-header__info__author__name">Steven Asarch</p>
    </a>
    
    <!-- published -->
    <div class="article-header__info__author__published">
              <time datetime="2023-07-26T17:53:10-05:00">
          Updated on July 26 2023 5:53 pm CDT        </time>
        
    </div>
  </div>
</section>

      
<section class="article-lead-image">
  <img class="single-article-top-img" style="aspect-ratio:2/1" alt="Article Lead Image" src="https://uploads.dailydot.com/2022/10/passionfruit-dream.jpg?q=65&amp;auto=format&amp;w=1600&amp;ar=2:1&amp;fit=crop" srcset="https://uploads.dailydot.com/2022/10/passionfruit-dream.jpg?q=65&amp;auto=format&amp;w=1600&amp;ar=2:1&amp;fit=crop 1200w,https://uploads.dailydot.com/2022/10/passionfruit-dream.jpg?q=65&amp;auto=format&amp;w=1200&amp;ar=2:1&amp;fit=crop 1000w,https://uploads.dailydot.com/2022/10/passionfruit-dream.jpg?q=65&amp;auto=format&amp;w=800&amp;ar=2:1&amp;fit=crop 800w" sizes="(max-width: 1135px) 100vw, 800px" loading="eager" pinger-seen="true">  <div class="article-lead-image__credit">
