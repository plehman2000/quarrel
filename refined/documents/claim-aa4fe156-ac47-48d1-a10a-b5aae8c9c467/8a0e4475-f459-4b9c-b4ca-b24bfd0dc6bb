            (function() {
                fetch(
                    'https://open-api.spot.im/v1/messages-count?spot_id=sp_BgbiFIw6&posts_ids=1040024',
                    { method: 'GET' }
                )
                .then(function(response){ return response.json() })
                .then(function(json){
                    if ( json ) {
                    var counts = json['messages_count'];
                        if (counts) {
                            var count = json['messages_count'][1040024];
                                                        if ( document.getElementById('comments-icon-1040024') ) {
                                document.getElementById('comments-icon-1040024').querySelector('.wp-block-gamurs-comment-button__number').textContent = count;
                            }
                        }
                    }
                })
            })();
