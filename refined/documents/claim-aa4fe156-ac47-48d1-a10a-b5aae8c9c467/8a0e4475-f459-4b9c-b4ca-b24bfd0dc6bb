<script>
  (function() {
    console.log('primis onpage - hi');

    function injectPrimisScript() {        
      console.log('primis onpage - inject');          
      var primisPlaceholder = document.querySelector('.primis-player:not(.no-gdpr)');
      console.log('primis onpage - ', primisPlaceholder, document.querySelector('#primisPlayerContainerDiv'));
      if (primisPlaceholder && !primisPlaceholder.querySelector('script') && !document.querySelector('#primisPlayerContainerDiv')) {
        var primisScript = document.createElement('script');
        var primisPlayerId = '103704';
				if (window.innerWidth < 768) {
									}
        console.log('primis onpage - ', primisPlayerId);
        if (primisPlayerId) {
          var primisScriptSrc = 'https://live.primis.tech/live/liveView.php?s=' + primisPlayerId + '&cbuster=%%CACHEBUSTER%%&playerApiId=primisPlayer&subId=minecraft';
          primisScript.src = primisScriptSrc;
          primisScript.id = "onpage-inject";
          primisPlaceholder.replaceChildren(primisScript);
          console.log('primis onpage - injected');
          if (window.snowplow) {
            if (
              primisPlaceholder.querySelector('script') 
              && primisPlaceholder.querySelector('script').src === primisScriptSrc
            ) {
              window.snowplow('trackStructEvent', {
                category: 'Primis Player', 
                action: 'Script Injected',
                label: primisPlayerId,
              });
            }
          }
        } else {
          console.log('primis onpage - no placeholder, or primis already loaded');
          if (window.snowplow) {              
            window.snowplow('trackStructEvent', {
              category: 'Primis Player', 
              action: 'Script Failed To Inject',
              label: !primisPlaceholder ? 'No Placeholder' : 'Already Loaded',
            });
          }
        }
      }
    }

    function loadPrimis() {     
      console.log('primis onpage - load');
      injectPrimisScript();
    }

    function gdprDetect() {
      console.log('primis onpage - gdpr');
      if (window.__tcfapi) {
        window.__tcfapi('getTCData', 2, function (tcData, success) {
          if (success) {
            console.log('primis onpage - got gdpr status', tcData);
            if (tcData.gdprApplies === true) {          
              console.log('primis onpage - gdpr does apply', tcData);
              window.__tcfapi('addEventListener', 2, function (tcData2, success) {
                if (success 
                    && (!tcData2.gdprApplies || (tcData2.gdprApplies && (tcData2.eventStatus === 'tcloaded' || tcData2.eventStatus === 'useractioncomplete')))) {
                  console.log('primis onpage - user interacted with CMP');
                  if ((tcData2.vendor.consents[228] || tcData2.vendor.legitimateInterests[228])) {                    
                    console.log('primis onpage - got gdpr consent for primis');
                  }
                  loadPrimis();
                }
              });
            } else {
              console.log('primis onpage - gdpr doesn\'t apply');
              loadPrimis();
            }
          }
        }, [228]);
      } else if (gdprChecks < 21) {
        var timeout = setTimeout(function() { gdprDetect(); gdprChecks++; }, 100);
      } else {
        console.log('primis onpage - timeout: couldn\'t find tcfapi');
        loadPrimis();
      }
    }

    fetch('https://geoip.gamurs.workers.dev/')
      .then(function(response) { return response.json() })
      .then(function(json) { 
        console.log('primis onpage - got geodata', json.continent);
        if (json.continent === "EU") {
          gdprDetect();
        } else {
          loadPrimis();
        }
      });
  })();
</script>


