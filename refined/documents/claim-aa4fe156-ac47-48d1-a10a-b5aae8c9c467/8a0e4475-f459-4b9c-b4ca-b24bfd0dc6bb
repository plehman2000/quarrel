        <script>
            (function() {
                var openComments = function() {
                    if ( ! document.querySelector( '[data-spotim-module="conversation"][data-post-id="1040024"]' ) ) {
                        var owDiv = document.createElement( 'div' );
                        owDiv.setAttribute( 'data-spotim-module', 'conversation' );
                        owDiv.setAttribute( 'data-spot-id', 'sp_BgbiFIw6' );
                        owDiv.setAttribute( 'data-post-id', '1040024' );
                                                owDiv.setAttribute( 'data-theme', window.localStorage.getItem('GAMURS_Wordpress-theme') || 'light' );
                        document.getElementById( 'comments-btn-1040024' ).parentNode.append( owDiv );
                    }
                }
                                if ( document.getElementById( 'comments-icon-1040024' ) ) {
                    document.getElementById( 'comments-icon-1040024' ).addEventListener('click', function() {
                        openComments();
                        if ( document.querySelector('#comment-button-scrollto-comments-btn-1040024') ) {
                            document.querySelector('#comment-button-scrollto-comments-btn-1040024')
                                .scrollIntoView({
                                    behavior: 'smooth'
                                });
                        }
                    });
                }
                if ( document.getElementById( 'comments-btn-1040024' ) ) {
                    document.getElementById( 'comments-btn-1040024' ).addEventListener('click', function() {
                        openComments();
                    });
                    var OWAd = document.createElement('div');
                    OWAd.setAttribute('data-openweb-ad', '');
                    OWAd.setAttribute('data-row', '1');
                    OWAd.setAttribute('data-column', '1');
                    if ( window.innerWidth > 992 ) {
                        document.querySelector('.is-sidebar-container > .wp-block-gamurs-column > .wp-block-gamurs-column:last-of-type').after(OWAd);                        
                    } else {
                        OWAd.style.marginBottom = '1rem';
                        document.getElementById( 'comments-btn-1040024' ).before(OWAd);
                    }
                                    }
            })();
        </script>
                
        
