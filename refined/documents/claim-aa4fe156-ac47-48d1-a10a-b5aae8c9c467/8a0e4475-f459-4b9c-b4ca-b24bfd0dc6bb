<script>
	window.gamursInfScrollPageViewsURLs = window.gamursInfScrollPageViewsURLs || [window.location.href];
	window.gamursHTLTargetingParams = window.gamursHTLTargetingParams || [];
	window.gamursHTLTargetingParams['https://dotesports.com/minecraft/news/dream-denies-new-grooming-allegation-claims-next-video-will-respond-to-everything'] = {
		post_id: '1040024',
		category: ["minecraft"],
		tag: []
	}

	window.gamursInfScrollOnUrlChange = function(permalink) {
		try {
			htlbid.cmd.push(() => {
				htlbid.setTargeting("post_id", window.gamursHTLTargetingParams[permalink].post_id);
				htlbid.setTargeting("category", window.gamursHTLTargetingParams[permalink].category); // dynamically pass page category into this function
				htlbid.setTargeting("tag", window.gamursHTLTargetingParams[permalink].tag); // dynamically pass unique post/page id into this function
				if (!htlbid.clientVersion.startsWith('3.')) {
					htlbid.newPage().then(() => {
						htlbid.reloadSlot('.htlad-dotesportscom_sticky_footer');
						Array.from(document.querySelectorAll('div.htlad-placeholder-dotesportscom_incontent')).forEach((placeholder) => {
							const real = document.createElement('div');
							real.className = "htlad-dotesportscom_incontent";
							placeholder.replaceWith(real);
						});
						Array.from(document.querySelectorAll('div.htlad-placeholder-dotesportscom_incontent_intable')).forEach((placeholder) => {
							const real = document.createElement('div');
							real.className = "htlad-dotesportscom_incontent_intable";
							placeholder.replaceWith(real);
						});
						Array.from(document.querySelectorAll('div.htlad-placeholder-dotesportscom_article_end')).forEach((placeholder) => {
							const real = document.createElement('div');
							real.className = "htlad-dotesportscom_article_end";
							placeholder.replaceWith(real);
						});
					});
				}
			});
		} catch(e) {
			console.warn(e);
		}

		window.gamursInfScrollPageViewsURLs = window.gamursInfScrollPageViewsURLs || [];
		if (!window.gamursInfScrollPageViewsURLs.includes(permalink)) {
			window.gamursInfScrollPageViewsURLs.push(permalink);

			try {
				if (htlbid.clientVersion.startsWith('3.')) {
					htlbid.cmd.push(() => {
						htlbid.newPage();
						Array.from(document.querySelectorAll('div.htlad-placeholder-dotesportscom_incontent')).forEach((placeholder) => {
							const real = document.createElement('div');
							real.className = "htlad-dotesportscom_incontent";
							real.classList.add("wp-block-gamurs-ad__creative");
							placeholder.replaceWith(real);
						});
						Array.from(document.querySelectorAll('div.htlad-placeholder-dotesportscom_incontent_intable')).forEach((placeholder) => {
							const real = document.createElement('div');
							real.className = "htlad-dotesportscom_incontent_intable";
							real.classList.add("wp-block-gamurs-ad__creative");
							placeholder.replaceWith(real);
						});
						Array.from(document.querySelectorAll('div.htlad-placeholder-dotesportscom_article_end')).forEach((placeholder) => {
							const real = document.createElement('div');
							real.className = "htlad-dotesportscom_article_end";
							real.classList.add("wp-block-gamurs-ad__creative");
							placeholder.replaceWith(real);
						});
					});
				}
			} catch(e) {
				console.log(e)
			}

			try {
				gtag('config', 'UA-86459307-1', {
					"page_title": "Dream denies new grooming allegation, claims next video will &#8216;respond to everything&#8217; - Dot Esports",
					"page_path": location.pathname,
					"dimension1": "Issy van der Velde",
					"dimension3": "November 21, 2023",
					"dimension4": "News",
					"dimension5": "&quot;minecraft&quot;",
					"dimension6": "direct",
					"dimension7": 0,
					"dimension8": "streaming",
					"dimension9": "",
				});
				console.log('googled');
			} catch (e) {
				console.log(e)
			}
			try {
				window.snowplow(
					'trackPageView',
										{ context: [              {
          schema: 'iglu:group.gamurs/article_context/jsonschema/1-0-0',
          data: {
            id: '1040024',
            authors: 'Issy van der Velde',
            authorIds: '2189',
            articleType: 'News',
            categories: 'Minecraft',
            publishedDate: '2023-11-21T23:59:00-05:00',
            lastModifiedDate: '2023-11-22T03:00:41-05:00',
            tags: '',
            title: 'Dream denies new grooming allegation, claims next video will &#8216;respond to everything&#8217;',
          }
        }
          ] }, 
									);
				console.log('plowed');
			} catch (e) {
				console.log(e)
			}
			try {
				pSUPERFLY.virtualPage({
					sections: '"minecraft"', //phpcs:ignore
