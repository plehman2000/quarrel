              var floatStatus = 0;
              var closeBtnTimeout = [];
              window.addEventListener('primisPlayerInit', function(e) {
                  var primisPlayer = e.detail;
                  primisPlayer.addEventListener('floatStatusChange', function(floatStatusValue) {
                      floatStatus = floatStatusValue;
                      if (floatStatus == 1) {
                          if (document.getElementById('closeIcon')) {
                              document.getElementById('closeIcon').style.display = 'none';
                          }
                          closeBtnTimeout.push(setTimeout(() => {
                              if (document.getElementById('closeIcon')) {
                                  document.getElementById('closeIcon').style.display = 'block';
                              }
                          }, 5000));
                      }
                      else {
                          var closeBtnTimeoutLen = closeBtnTimeout.length;
                          closeBtnTimeout.forEach(to => {
                              clearTimeout(to);
                          })
                          closeBtnTimeoutLen.splice(0, closeBtnTimeoutLen);
                      }
                  });
              });

              function adjustPlayer(adEl, player, interval) {
                  var adElHeight = adEl.getBoundingClientRect().height;
                  var bottomAd = document.querySelector('#bottom-adhesion');
                  var bottomAdHeight = bottomAd ? bottomAd.getBoundingClientRect().height : 0;
                  if (bottomAdHeight > adElHeight)
                      adElHeight = bottomAdHeight;
                  if (floatStatus == 1) player.style.bottom = adElHeight + "px";
              }

              function selectDivs() {
                  var selectors = ['#cc-collapsed', '.kargo-ad-hover'];

                  var selectedDiv = null;
                  selectors.forEach(function(selector) {
                      selectedDivResult = document.querySelector(selector);
                      if (selectedDivResult != null)
                          selectedDiv = selectedDivResult;
                  });

                  return selectedDiv;
              }

              function detectAd() {
                  var primisPlayer = document.querySelector('[id^="primis_playerSekindoSPlayer"]');
                  var isMobile = window.innerWidth < 600 ? true : false;
                  var adDiv = selectDivs();

                  if (adDiv != null) {
                      if (floatStatus == 1) {
                          clearInterval(detectionInterval);
                          var adjustPlayerInterval = setInterval(function() {
                              adjustPlayer(adDiv, primisPlayer, adjustPlayerInterval)
                          }, 200);
                      }
                  }
              }

              var detectionInterval = setInterval(detectAd, 200);
