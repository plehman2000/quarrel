                        function tracking() {
                var post_id = $(".post_id").val();
                var utm_source = $(".utm_source").val();
                var utm_medium = $(".utm_medium").val();
                var t = $.now();
                $.ajax({
                    url: '/ajax/tracking',
                    params: {
                        _token: "65W8bBS94ZL4XdhZxGgEjcCBSsHDLW7DTf2tJMSx"
                    },
                    headers: {
                        'X-CSRF-TOKEN': "65W8bBS94ZL4XdhZxGgEjcCBSsHDLW7DTf2tJMSx"
                    },
                    data: {
                        post_id : post_id,
                        utm_medium : utm_medium,
                        utm_source : utm_source,
                        t : t
                    },
                    method: 'GET',
                    dataType: 'json',
                    success: function(data){
                    }
                });
            }
